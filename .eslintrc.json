{
    "env": {
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "plugins": [
        "import"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, 4],
        "no-underscore-dangle": 0,
        "no-multi-spaces": 0,
        "no-console": 0,
        "quotes": [2, "single", { "avoidEscape": true }],
        "import/no-unresolved": 2,
        "import/named": 2,
        "import/namespace": 2,
        "import/default": 2,
        "import/export": 2,
        "import/extensions": 0,
        "no-duplicate-imports": "error",
        "class-methods-use-this": 0,
        "no-undef": 0,
        "max-len": 0
    }
}